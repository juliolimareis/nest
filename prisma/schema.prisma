generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL_MYSQL_01")
}

model student {
  id        Int       @id @default(autoincrement())
  name      String
  birthday  DateTime?
  createdAt DateTime  @default(now()) @db.Timestamp(0)
}
